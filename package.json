{
    "name": "aistudio-to-api",
    "version": "0.1.0",
    "author": "Ellinav",
    "description": "Encapsulate Gemini models on AI Studio as API services.",
    "main": "main.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon main.js",
        "start": "cross-env NODE_ENV=production node main.js",
        "save-auth": "node scripts/saveAuth.js",
        "setup-auth": "scripts\\setupAuth.bat",
        "prepare": "husky install",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "dependencies": {
        "basic-auth": "^2.0.1",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-session": "^1.18.0",
        "playwright": "^1.53.1",
        "ws": "^8.17.0"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "eslint": "^8.57.1",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "husky": "^8.0.3",
        "lint-staged": "^14.0.0",
        "nodemon": "^3.1.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": "eslint --fix"
    }
}
